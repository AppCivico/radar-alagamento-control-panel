{
  "name": "radar-alagamento-control-panel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "admin-lte": "^2.4.2",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/stylesheets/ -o src/",
    "build:js": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p watch:css build:js",
    "test": "react-scripts test --env=jsdom",
    "watch:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/stylesheets/ -o src/ --watch --recursive"
  },
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
