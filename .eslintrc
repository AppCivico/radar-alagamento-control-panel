{
  "extends": ["react-app", "airbnb"],
  "globals": {
    "chai": true,
    "context": true,
    "cy": true
  },
  "rules": {
    "no-tabs": 0,
    "indent": ["error", "tab"],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-indent": [2, "tab"],
    "react/jsx-indent-props": [2, "tab"],
    "jsx-a11y/href-no-hash": 0,
    "class-methods-use-this": 0
  }
}
